@page "/build"
@inject IBananaService BananaService

<h3>Build</h3>

<button class="btn btn-primary" @onclick="() => EatBananas(10)">Eat Bananas</button>

@code {
    private void EatBananas (int amount)
    {
        //Console.WriteLine($"{amount} bananas have been eaten");

        BananaService.EatBananas(amount);
    }
}
